#include<iostream>
#include<string.h>
#include<fstream>
using namespace std;

int rot270(int n,string ini[],string final[]){
    int count = 0,i,j;
    for(i=0;i<n;i++){
        for(j=0;j<n;j++){
            if(ini[i][j] == final[n-1-j][i]){
                count++;
            }
        }
    }
    return count;
}

int rot180(int n,string ini[],string final[]){
    int count = 0,i,j;
    for(i=0;i<n;i++){
        for(j=0;j<n;j++){
            if(ini[i][j] == final[n-1-i][n-1-j]){
                count++;
            }
        }
    }
    return count;
}

int rot90(int n,string ini[],string final[]){
    int count = 0,i,j;
    for(i=0;i<n;i++){
        for(j=0;j<n;j++){
            if(ini[i][j] == final[j][n-1-i]){
                count++;
            }
        }
    }
    return count;
}

int same(int n,string ini[],string final[]){
    int count = 0,i,j;
    for(i=0;i<n;i++){
        for(j=0;j<n;j++){
            if(ini[i][j] == final[i][j]){
                count++;
            }
        }
    }
    return count;
}


int main(){
   
    ifstream in("transform.in");
    ofstream out("transform.out");
    
    int i,j,n;
    
    in >> n;
    
    string ini[n],final[n],ref[n];
    
    for(i=0;i<n;i++){
        in >> ini[i];
        ref[i] = ini[i];
        for(j=0;j<n;j++){
            ref[i][j] = ini[i][n-1-j];
        }
    }
    
    for(i=0;i<n;i++){
        in >> final[i];
    }
    
    if(rot90(n,ini,final)==n*n){
        out << 1 <<endl;
    }
    else if(rot180(n,ini,final)==n*n){
        out << 2 <<endl;
    }
    else if(rot270(n,ini,final)==n*n){
        out << 3 <<endl;
    }
    else if(same(n,ref,final)==n*n){
        out << 4 <<endl;
    }
    else if(rot90(n,ref,final)==n*n || rot180(n,ref,final)==n*n || rot270(n,ref,final)==n*n){
        out << 5 <<endl;
    }
    else if(same(n,ini,final)==n*n){
        out << 6 <<endl;
    }
    else{
        out << 7 <<endl;
    }
    
    return 0;
 }
    

