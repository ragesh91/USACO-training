#include<iostream>
#include<string.h>
#include<fstream>
#include <sstream>
using namespace std;

bool palind(string x){
    int len = x.length(),i;
    
    for(i=0;i<len/2;i++){
        if(x[i]!=x[len-1-i])
            return false;
    }
    return true;
}

int main(){
   
    ifstream in("dualpal.in");
    ofstream out("dualpal.out");
    
    int i,n,s,rem,num,count;
    string conv="";
    stringstream conversion;
    
    in >> n >> s;
    
    while(n){
        s++;
        count = 0;
        for(i=10;i>1;i--){
            num = s;
            conv = "";
            while(num){
                rem = num % i;
                num /= i;
                conversion << rem;
                conv += conversion.str();
                conversion.str(string());
                conversion.clear();
            }
            if(palind(conv))
                count++;
            
            if(count == 2)
                break;
        }
        
        if(count == 2){
            out<<s<<endl;
            n--;
        }
    }
    
    
    return 0;
 }
    

