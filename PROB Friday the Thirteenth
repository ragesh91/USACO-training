#include<iostream>
#include<string.h>
#include<fstream>
using namespace std;

bool isleep(int year){
    if(year%100==0){
        if(year%400==0){
            return true;
        }
        else
            return false;
    }
    else if(year%4==0)
        return true;
    else
        return false;
}

int main(){
   
    ifstream in("friday.in");
    ofstream out("friday.out");
    
    int N,i,num[7]={0,0,0,0,0,0,0},days[12]={31,28,31,30,31,30,31,31,30,31,30,31};
    
    in >> N;
    
    int mon;
    int year = 1900;
    
    int last = 0;
    
    for(i=0;i<N;i++){
        for(mon=0;mon<12;mon++){
            if(mon==0 && i==0){
                num[last]++;
            }
            else if(mon==0){
                last += days[11];
                last = last%7;
                num[last]++;
            }
            else if(mon==2 && isleep(year+i)){
                last += days[mon-1]+1;
                last %= 7;
                num[last]++;
            }
            else{
                last += days[mon-1];
                last %= 7;
                num[last]++;
            }
        }
    }
    
    for(i=0;i<7;i++){
        out << num[i];
        if(i!=6)
            out<< " ";
    }
    
    out <<endl;
    
    return 0;
 }
    

